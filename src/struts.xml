<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
   "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>

<package name="default" extends="struts-default">

<action name="login" class="com.javatpoint.LoginAction" method="login">  
<result name="success">success.jsp</result>
<result name="error">fail.jsp</result>
</action>

<action name="logout" class="com.javatpoint.LoginAction" method="logout">  
<result name="success">success.jsp</result>
<result name="error">fail.jsp</result>
</action>

<action name="register" class="com.javatpoint.RegisterAction">
<result name="input">register.jsp</result>
<result name="success">success.jsp</result>
<result name="error">fail.jsp</result>
</action>

<action name="profile" class="">
<result name="success">success.jsp</result>
<result name="error">fail.jsp</result>
</action>

</package>
</struts>    
